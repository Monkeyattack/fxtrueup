{
  "name": "metatrader-reporting",
  "version": "2.0.0",
  "description": "Optimized MetaTrader portfolio tracking with advanced caching and performance monitoring",
  "type": "module",
  "main": "server.cjs",
  "scripts": {
    "start": "node server.cjs",
    "start:legacy": "node server-commonjs.cjs",
    "start:secure": "node server-secure.cjs",
    "start:optimized": "node server-optimized.cjs",
    "dev": "NODE_ENV=development nodemon server.cjs",
    "production": "NODE_ENV=production node server.cjs",
    "test:performance": "node performance-test.js",
    "cache:stats": "node -e \"const cache = require('./sqlite-cache-optimized.cjs'); cache.getStatistics().then(console.log)\"",
    "cache:warmup": "node -e \"const service = require('./metaapi-service-optimized.cjs'); service.warmupCache(['account-id-here'])\"",
    "lint": "eslint *.js public/js/*.js",
    "migrate:cache": "node migrate-cache.js",
    "status": "node src/api/status-server.mjs"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "metaapi.cloud-sdk": "^27.0.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "redis": "^5.8.2",
    "sqlite3": "^5.1.6",
    "stripe": "^18.4.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "metatrader",
    "forex",
    "trading",
    "portfolio",
    "metaapi",
    "performance-optimization",
    "caching"
  ],
  "author": "C. Meredith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monkeyattack/metatrader-reporting.git"
  },
  "bugs": {
    "url": "https://github.com/Monkeyattack/metatrader-reporting/issues"
  },
  "homepage": "https://fxtrueup.com"
}
