{
  "name": "metatrader-reporting",
  "version": "2.0.0",
  "description": "Optimized MetaTrader portfolio tracking with advanced caching and performance monitoring",
  "main": "server-optimized.cjs",
  "scripts": {
    "start": "node server-optimized.cjs",
    "start:legacy": "node server-commonjs.cjs",
    "dev": "NODE_ENV=development nodemon server-optimized.cjs",
    "dev:legacy": "NODE_ENV=development nodemon server-commonjs.cjs",
    "production": "NODE_ENV=production node server-optimized.cjs",
    "test:performance": "node performance-test.js",
    "cache:stats": "node -e \"const cache = require('./sqlite-cache-optimized.cjs'); cache.getStatistics().then(console.log)\"",
    "cache:warmup": "node -e \"const service = require('./metaapi-service-optimized.cjs'); service.warmupCache(['account-id-here'])\"",
    "lint": "eslint *.js public/js/*.js",
    "migrate:cache": "node migrate-cache.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "metaapi.cloud-sdk": "^27.0.2",
    "sqlite3": "^5.1.6",
    "compression": "^1.7.4",
    "cluster": "^0.7.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "metatrader",
    "forex",
    "trading",
    "portfolio",
    "metaapi",
    "performance-optimization",
    "caching"
  ],
  "author": "C. Meredith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monkeyattack/metatrader-reporting.git"
  },
  "bugs": {
    "url": "https://github.com/Monkeyattack/metatrader-reporting/issues"
  },
  "homepage": "https://fxtrueup.com"
}